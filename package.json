{
  "name": "launch-goblin",
  "version": "0.1.0",
  "main": "./dist/index.cjs",
  "type": "commonjs",
  "bin": {
    "launch-goblin": "dist/index.cjs"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@whimbrel/core": "0.0.3",
    "@whimbrel/npm": "0.0.3",
    "@whimbrel/package-json": "0.0.3",
    "@whimbrel/node": "0.0.3",
    "@whimbrel/turborepo": "0.0.3",
    "neo-blessed": "^0.2.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
